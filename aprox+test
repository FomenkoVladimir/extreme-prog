#include <iostream>
#include <conio.h>

using namespace std;
const int n=4;
const float d=0.01;


float sqr(float a){
	return(a*a);
}

float fa(float a[n][2],int n){
	float sumx,sumy,sumxy,sumxx;
	sumx=0;
	sumy=0;
	sumxy=0;
	sumxx=0;
	for(int i=0;i<n;i++){
		sumx+=a[i][0];
		sumy+=a[i][1];
		sumxy+=a[i][0]*a[i][1];
		sumxx+=sqr(a[i][0]);
	}
  	return ((sumx*sumy-n*sumxy)/(sumx*sumx-n*sumxx));
}
float fb(float a[n][2],int n){
	float sumx,sumy,sumxy,sumxx;
	sumx=0;
	sumy=0;
	sumxy=0;
	sumxx=0;
	for(int i=0;i<n;i++){
		sumx+=a[i][0];
		sumy+=a[i][1];
		sumxy+=a[i][0]*a[i][1];
		sumxx+=sqr(a[i][0]);
	}
	return((sumx*sumxy-sumxx*sumy)/(sqr(sumx)-n*sumxx));
}

int test(float a[n][2],int n){
	float arr[n][2];
	for(int i=0;i<n;i++){
		arr[i][0]=a[i][0];
		arr[i][1]=a[i][1];
	}
	float aa=fa(a,n);
	float bb=fb(a,n);
	float a2=-(1/aa);
	for(int i=0;i<n;i++){
		float bi=arr[i][1]-a2*arr[i][0];
		arr[i][0]+=1;
		arr[i][1]=a2*arr[i][0]+bi;
	}
		for(int i=0;i<n;i++){
		cout<<'{'<<arr[i][0]<<','<<arr[i][1]<<'}'<<' ';
	}
	cout<<fa(arr,n)<<endl<<fb(arr,n)<<endl;
	if((fa(a,n)<=fa(arr,n)+d)&&(fa(a,n)>=fa(arr,n)-d))return(1);
	return(0);
}
	

int main(){
	float a[n][2];
	for(int i=0;i<n;i++){
		cin>>a[i][0];
		cin>>a[i][1];
	}
	for(int i=0;i<n;i++){
		cout<<'{'<<a[i][0]<<','<<a[i][1]<<'}'<<' ';
	}
	cout<<endl;
	cout<<"a="<<fa(a,n)<<endl;
	cout<<"b="<<fb(a,n)<<endl;
	if(test(a,n)==1)cout<<"test complited";
	else cout<<"test failed";
	_getch();
	return(0);
}
